{
  "request": {
    "method": "POST",
    "urlPath": "/checkAppAccess",
    "queryParameters": {
      "token": {
        "matches": ".*"
      }
    },
    "bodyPatterns": [
      {
        "matchesJsonPath": {
          "expression": "$.username",
          "matches": "^(edevries|sdegraaf|tvos)$"
        }
      },
      {
        "matchesJsonPath": {
          "expression": "$.application",
          "equalTo": "triptop"
        }
      }
    ]
  },
  "response": {
    "status": 200,
    "jsonBody": {
      "access": "allowed"
    }
  }
}
